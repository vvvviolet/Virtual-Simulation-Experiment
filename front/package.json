{
  "name": "stepin-template",
  "private": true,
  "version": "1.0.0-preview",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "node service/index.js",
    "build": "vue-tsc --noEmit && vite build",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "preview": "vite preview",
    "deploy": "yarn build && gh-pages -d dist -b pages -r https://gitee.com/stepui/stepin-template.git",
    "deploy:github": "yarn build --mode github && gh-pages -d dist -b master -r git@github.com:stepui/stepui.github.io.git",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:preview": "vitepress preview docs",
    "docs:deploy": "gh-pages -d docs/.vitepress/dist -b main -r https://gitee.com/stepui/stepin-template-docs.git"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.15",
    "axios": "^0.21.1",
    "clipboard": "^2.0.8",
    "cross-env": "^7.0.3",
    "d3": "6",
    "dayjs": "^1.11.6",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.3.3",
    "element-plus": "^2.3.2",
    "element-ui": "^2.15.13",
    "enquire.js": "^2.1.6",
    "html2canvas": "^1.4.1",
    "increase-memory-limit": "^1.0.7",
    "js-cookie": "^2.2.1",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "qs": "^6.10.1",
    "stepin": "^2.1.38-beta",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
