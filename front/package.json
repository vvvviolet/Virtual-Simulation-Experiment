    "mockjs": "^1.1.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.24.1",
    "util": "^0.12.3",
    "vite": "^4.2.1",
    "vitepress": "^1.0.0-alpha.61",
    "vue-tsc": "^1.0.13"
  }
